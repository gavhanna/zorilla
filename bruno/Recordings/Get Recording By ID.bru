meta {
  name: Get Recording By ID
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/api/recordings/{{recordingId}}
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

vars:post-response {
  recordingId: res.body.id
}

assert {
  res.status: eq 200
  res.body.id: isString
  res.body.title: isString
}
