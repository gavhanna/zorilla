meta {
  name: Register Admin
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/api/auth/register
  body: json
  auth: none
}

body:json {
  {
    "name": "Admin User",
    "email": "admin@example.com",
    "password": "password123",
    "role": "admin"
  }
}

vars:post-response {
  token: res.body.token
}

assert {
  res.status: eq 201
  res.body.user.role: eq admin
  res.body.user.email: eq admin@example.com
}
