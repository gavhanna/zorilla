meta {
  name: Register
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/auth/register
  body: json
  auth: none
}

body:json {
  {
    "name": "Test User",
    "email": "test@example.com",
    "password": "password123"
  }
}

vars:post-response {
  token: res.body.token
}

assert {
  res.status: eq 201
  res.body.token: isString
  res.body.user.email: eq test@example.com
}
