meta {
  name: Login
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/api/auth/login
  body: json
  auth: none
}

body:json {
  {
    "email": "test@example.com",
    "password": "password123"
  }
}

vars:post-response {
  token: res.body.token
}

assert {
  res.status: eq 200
  res.body.token: isString
  res.body.user.email: eq test@example.com
}
